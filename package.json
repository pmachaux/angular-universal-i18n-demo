{
  "name": "angular-universal-i18n-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run angular-universal-i18n-demo:serve-ssr",
    "serve:ssr": "node dist/angular-universal-i18n-demo/proxy-server.js",
    "build:ssr": "ng build --prod && ng run angular-universal-i18n-demo:server:production && cpx proxy-server.js dist/angular-universal-i18n-demo",
    "prerender": "ng run angular-universal-i18n-demo:prerender",
    "extract-i18n": "ng xi18n angular-universal-i18n-demo --i18n-format xlf --output-path src/i18n --i18n-locale en && ng run angular-universal-i18n-demo:xliffmerge",
    "start-fr": "ng serve --configuration=fr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.1.0",
    "@angular/localize": "^9.1.9",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/platform-server": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@nguniversal/express-engine": "^9.1.1",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "express": "^4.15.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@nguniversal/builders": "^9.1.1",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
